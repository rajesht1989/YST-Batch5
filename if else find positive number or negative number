//write a program get number from user and find positive number or nagative number and print it on console

#include <stdio.h>

int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int positiveOrNagative(int a){
if (a>0){
    printf("This Number Is Positive");
}
else if(a<0){
    printf("This Number Is Nagative");
}
else {
    printf("Nutral");
}
}
void main(){
printf("Enter The Number ");
int a=getNumberFromUser();

int PositiveNagative=positiveOrNagative(a);
 }
