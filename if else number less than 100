//write a program get a number from user and identify if the number is <100

#include <stdio.h>

int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyTheNumber(int a){
if (a<100){
    printf("This Number Is Correct");
}
else {
    printf("This Number Is Not Correct");
}
}
void main(){
printf("Enter The Number ");
int a=getNumberFromUser();
int value=identifyTheNumber(a);
}
