//get number from user and print holo square star print it on console

#include<stdio.h>
 
 int getNumberFromUser(){
     int number;
     printf("Enter The number ");
     scanf("%d",&number);
     return number;
 }
int heloSquarePattern(int a){
    for(int i=1;i<=a;i++){
        for(int j=1;j<=a;j++){
            if(i == 1 || j == 1 || i == a || j == a ){
         printf("*\t");
}else{
    printf("\t");
}
                   }
      printf("\n");
    } 
}
void main(){
    heloSquarePattern(getNumberFromUser());
}
