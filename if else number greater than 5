//write a program get a number from user and identify if the number is >5

#include <stdio.h>

int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyTheNumber(int a){
if (a>5){
printf ("This Number Is correct");
}
else {
    printf("this Number Is Not Correct");
}}
void main(){
    printf("Enter The Number ");
int a=getNumberFromUser();
int value=identifyTheNumber(a);
}
