//wirte a program get two number from user and identify largest number and print it on console.

#include <stdio.h>

int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyTheLargestNumber(int a,int b){
if(a>b){
    printf("First Number Is Largest Number");
}
else{
    printf("The Second Number Is Largest Number");
}
}
void main(){
printf("Enter The First Number ");
int a=getNumberFromUser();
printf("Enter the Second Number ");
int b=getNumberFromUser();

int identify=identifyTheLargestNumber(a,b);
}
