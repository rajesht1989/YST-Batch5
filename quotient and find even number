//write a program and get two number from user and identify the quotient and find even number

#include <stdio.h>

int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int quotient(int a,int b){
return a/b;
}
int findEvenNumber(int c){
if (c%2==0){
    printf("This Number Is Even Number");
}
else{
    printf("This Number Is Not Even Number");
}
}
void main(){
printf("Enter The First Number ");
int a=getNumberFromUser();
printf("Enter The Second Number ");
int b=getNumberFromUser();
int c=quotient(a,b);

printf("Answer Is %d\n",c);

findEvenNumber(c);
}
