//get Number from user those many times get number and add them print it on console

#include<stdio.h>

int getNumber(){
int n;
scanf("%d",&n);
return n;
}
int thoseManyTimes(int a){
   int addition=0;
   for(int i=1;i<=a;i++){
    printf("Enter The Number %d\n",i);
     addition+=getNumber();
   }
printf("The Addition Is %d \n",addition);
}

void main(){
    printf("Enter The Number ");
int a=getNumber();
int addition=thoseManyTimes(a);
}
