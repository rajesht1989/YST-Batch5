//get number from user and print rectengle star print it on console

#include<stdio.h>
 
 int getNumberFromUser(){
     int number;
     printf("Enter The number ");
     scanf("%d",&number);
     return number;
 }
int squareStarPattern(int n){
    for(int i=1;i<=n;i++){
     for(int j=1;j<=n*2;j++){
     printf("*\t");
     }
    printf("\n");
 }
 }
void main(){
    squareStarPattern(getNumberFromUser());
}
