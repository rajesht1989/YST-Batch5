//write a program get indiger from user and identify the number is divisible by 10

#include <stdio.h>

int getnumberfromUser(){
int number;
scanf("%d",&number);
return number;
}

int divisible(int a){
if (a%10==0){
printf("This Number Divisible");
}
else {
    printf("This Number Is Not Divisible");
}
}
void main(){
printf("Enter The Number ");
int a=getnumberfromUser();

int divisibleNumber=divisible(a);
}
