//get number from user and print square star print it on console

#include<stdio.h>
 
 int getNumberFromUser(){
     int number;
     printf("Enter The number ");
     scanf("%d",&number);
     return number;
 }
int squareStarPattern(int n){
     for(int j=1;j<=n;j++){
         for(int i=1;i<=n;i++){
     printf("*\t");
     }
    printf("\n");
 }
 }
void main(){
    squareStarPattern(getNumberFromUser());
}
