//write a program get three indiger from user and identify the colour of RGB

#include <stdio.h>

int getNumberFromUser(){
    int number;
    scanf("%d",&number);
    return number;
}
int findTheColour(int a,int b,int c){
    if(a>0&&a<255||b>0&&b<255||c>0&&c<255){
      printf("Invalid Number");  
    }
else if(a==0&&b==0&&c==0){
    printf("Black");
}
else if(a==255&&b==255&&c==255){
    printf("White");
}
else if(a==0&&b==0&&c==255){
    printf("Red");
}
}
void main(){
    printf("Enter The First Number ");
    int a=getNumberFromUser();
    printf("Enter The Second Number ");
    int b=getNumberFromUser();
    printf("Enter The Third Number ");
    int c=getNumberFromUser();
    
    int TheColour=findTheColour(a,b,c);
}
