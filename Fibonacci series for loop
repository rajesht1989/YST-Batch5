//get a number from user and identify the Fibonacci series
# include<stdio.h>
int getANumberFromUser(){
    int number;
    printf("Enter a number ");
    scanf("%d",&number);
    return number;
    }
    int identifyFibo(int n){
     int f1 = 0,f2 = 1,f3;
         for(int i = 1;i <= n;i++){
           f3 = f1 + f2;
             printf("%d ",f1);
             f1 = f2;
             f2 = f3;
               
         }
     }
    
    
  void main(){
   int n = getANumberFromUser();
   identifyFibo(n);
  }
